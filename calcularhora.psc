Algoritmo calcularhora
	
	
	definir horas,minutos,seg como entero;
	Definir  totalseg Como Entero;
	
	Escribir "DIGITE HORAS";
	Leer horas;
	Escribir  "DIGITE MINUTOS";
	Leer minutos;
	Escribir  "DIGITE SEGUNDOS";
	Leer  seg;
	
	totalseg <- (horas*3600) + (minutos*60) + seg;
	
	Escribir  "el total de segundos es : ", totalseg;
	
	
	
FinAlgoritmo
